[flake8]

# E111, E114: I know the indent is weird
# N802: I know the function names aren't recommended
# D413: Not sure what "Missing blank line after last section" means but it's not important
# The remaining ones are to get autopep8 to not modify the weird indent.
ignore = E111, E114, N802, D413, E101, E11, E127, E128, E129

max-line-length = 80
